{
  "name": "blacklinecloud/gowa-php-sdk",
  "description": "Type-safe PHP SDK for Go WhatsApp Web MultiDevice (REST & webhooks)",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/blacklinecloud/gowa-php-sdk",
  "require": {
    "php": ">=8.2",
    "psr/http-client": "^1.0",
    "psr/http-factory": "^1.0",
    "psr/log": "^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.2",
    "phpstan/phpstan": "^1.11",
    "vimeo/psalm": "^6.0",
    "friendsofphp/php-cs-fixer": "^3.66",
    "nikic/php-parser": "^5.4",
    "nyholm/psr7": "^1.8"
  },
  "autoload": {
    "psr-4": {
      "BlacklineCloud\\SDK\\GowaPHP\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "BlacklineCloud\\SDK\\GowaPHP\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "phpunit",
    "lint": "php-cs-fixer fix --dry-run --diff",
    "fix": "php-cs-fixer fix",
    "stan": "phpstan analyse",
    "psalm": "psalm",
    "coverage": "bin/check-coverage build/logs/clover.xml 90"
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "platform": {
      "php": "8.3.0"
    }
  }
}
